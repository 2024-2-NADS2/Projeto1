BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "consultas" (
	"id_consulta"	TEXT NOT NULL UNIQUE,
	"id_paciente"	TEXT NOT NULL UNIQUE,
	"data_consulta"	NUMERIC NOT NULL,
	"horario_consulta"	NUMERIC NOT NULL,
	PRIMARY KEY("id_consulta")
);
CREATE TABLE IF NOT EXISTS "paciente" (
	"cpf"	NUMERIC NOT NULL UNIQUE,
	"nome"	TEXT NOT NULL,
	"data_nascimento"	NUMERIC NOT NULL,
	"genero"	TEXT NOT NULL,
	"telefone"	NUMERIC NOT NULL,
	"e-mail"	TEXT NOT NULL,
	PRIMARY KEY("cpf")
);
CREATE TABLE IF NOT EXISTS "exames" (
	"id_exame"	NUMERIC NOT NULL,
	"id_paciente"	TEXT NOT NULL,
	"data_exame"	NUMERIC NOT NULL,
	"resultado"	TEXT NOT NULL,
	PRIMARY KEY("id_exame")
);
CREATE TABLE IF NOT EXISTS "medicamentos" (
	"id_medicamento"	NUMERIC NOT NULL,
	"nome_medicamento"	TEXT NOT NULL,
	PRIMARY KEY("nome_medicamento")
);
CREATE TABLE IF NOT EXISTS "agendamento" (
	"id_agendamento"	TEXT NOT NULL,
	"id_paciente"	TEXT NOT NULL,
	"data_agendamento"	NUMERIC NOT NULL,
	"status"	TEXT NOT NULL,
	PRIMARY KEY("id_agendamento")
);
COMMIT;
